/*
@author: huynhnv
@description: Changes display size screen
*/
  
#global_wrapper
{
  text-align: center;
  padding-top: 0px;
  background: url(~/application/modules/Core/externals/images/bg-content.gif) repeat-x; padding: 0px;  
}
#global_content
{
    width: $theme_content_width;
    margin: 0 auto 0 auto;
    min-height: 500px;
    padding-top:  20px;
    padding-bottom: 10px;
    font-size: 0.75em;
